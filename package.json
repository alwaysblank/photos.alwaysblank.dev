{
  "name": "web-starter",
  "version": "1.2.1",
  "description": "Basic web starter kit with 11ty, TailwindsCSS and PostCSS.",
  "scripts": {
    "assets:watch": "parcel src/_assets/styles/app.css src/_assets/scripts/app.js -d src/static",
    "assets:build": "parcel build src/_assets/styles/app.css src/_assets/scripts/app.js -d src/static --no-source-maps",
    "site:build": "eleventy build",
    "site:serve": "eleventy --serve",
    "build": "npm run assets:build && npm run site:build"
  },
  "author": "Chris Collins",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.6.1",
    "basiclightbox": "^5.0.2",
    "cloudinary": "^1.16.0",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^2.5.2",
    "markdown-it": "^10.0.0",
    "parcel-bundler": "^1.12.4",
    "postcss-import": "^12.0.1",
    "sass": "^1.23.7",
    "tailwindcss": "^1.1.4",
    "vanilla-lazyload": "^12.3.0"
  }
}
